package Übung2;

public class ButtonModel {
	private int counterLeft = 0;
	private int counterRight = 0;

	public void incCounterLeft() {
		counterLeft++;
	}

	public void incCounterRight() {
		counterRight++;
	}
	public int getLeftCounter(){
		return counterLeft;
	}
	public int getRightCounter(){
		return counterRight;
	}

}
